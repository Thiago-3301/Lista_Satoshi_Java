import javax.swing.JOptionPane;
public class Exercicio10 {
	public static void main(String [] arg) {
		int n1;
		int n2;
		int diferenca = 0;
		
		n1 = Integer.parseInt(JOptionPane.showInputDialog("Insira um número"));
		n2 = Integer.parseInt(JOptionPane.showInputDialog("Insira um outro número"));
		
		if(n1 > n2) {
			diferenca = n1 - n2;	
		}
		if(n2 > n1) {
			diferenca = n2 - n1;
		}
		
		System.out.print("A diferença dos dois números é: " + diferenca);
		
	
	}
}
