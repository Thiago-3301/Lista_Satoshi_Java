import javax.swing.JOptionPane;
public class Exercicio14 {
	public static void main(String[] args) {
		
		int ang1 = Integer.parseInt(JOptionPane.showInputDialog("insira o tamanho de um ângulo do trinângulo"));
		int ang2 = Integer.parseInt(JOptionPane.showInputDialog("insira o tamanho de outro ângulo do trinângulo"));
		
		int ang3 = 180 - ang2 - ang1;
		
		System.out.println("O valor do 3° ângulo é " + ang3);
		
		
	}
}
