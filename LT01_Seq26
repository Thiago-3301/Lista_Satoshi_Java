import javax.swing.JOptionPane;
public class Exercicio26 {
    public static void main(String[] args){
        
        int n1 = Integer.parseInt(JOptionPane.showInputDialog("Digite um numero"));
        int n2 = Integer.parseInt(JOptionPane.showInputDialog("Digite um outro numero"));
        int maior;
        int menor;
        
        if(n1 > n2){
            maior = n1;
            menor = n2;
        }else{
            maior = n2;
            menor = n1;
        }
        
        if(maior % menor == 0){
            System.out.print("O numero " + maior + " é divisivel por " + menor);
        }else{
            System.out.print("O numero " + maior + " não é divisivel por " + menor);
        }
        
    }
    
}
